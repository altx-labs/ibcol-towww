{
  "version": 2,
  "name": "ibcol-towww",
  "alias": ["ibcol.org"],
  "regions": ["all"],
  "github": {
    "enabled": true,
    "autoAlias": true,
    "silent": false,
    "autoJobCancelation": true
  },
  "routes": [
    {
      "src": "/(.*)",
      "status": 301,
      "headers": { "Location": "https://www.ibcol.org/$1" }
    }
  ]
}